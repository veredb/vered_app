
<h1>article here</h1>


<% @users.each do |user| %>

<%= user.name %><%= user.comment %><br />

<% end %>


<%= form_for(@user) do |f| %>
    <%= f.label :name %><br />
    <%= f.text_field :name %><br />
    <%= f.label :comment %><br />
    <%= f.text_area :comment %><br />
    <%= f.submit 'submit' %>
<% end %>

